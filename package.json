{
  "name": "elmstagram",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack -p --config config/webpack.prod.js",
    "clean": "rimraf dist",
    "postinstall": "elm package install -y",
    "prebuild": "npm run clean",
